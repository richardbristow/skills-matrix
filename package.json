    "react-bootstrap": "^1.0.0-beta.11",
    "react-dom": "^16.9.0",
    "react-feather": "^2.0.3",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.0",
    "styled-components": "^4.3.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^9.1.0",
    "eslint": "6.1.0",
    "eslint-config-airbnb": "18.0.0",
