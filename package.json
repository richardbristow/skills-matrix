{
  "name": "skills-matrix",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env REACT_APP_STAGE=dev react-scripts start",
    "build": "cross-env REACT_APP_STAGE=dev react-scripts build",
    "build:prod": "cross-env REACT_APP_STAGE=prod react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint 'src/**/*.{js,jsx}' 'backend/**/*.js' --ignore-pattern 'node_modules/*' && stylelint './src/**/*.{js,jsx}'",
    "lint:js": "eslint 'src/**/*.{js,jsx}' 'backend/**/*.js' --ignore-pattern 'node_modules/*'",
    "lint:css": "stylelint './src/**/*.{js,jsx}'"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!src/serviceWorker.js",
      "!src/setupTests.js",
      "!src/index.jsx"
    ]
  },
  "dependencies": {
    "aws-amplify": "^1.1.36",
    "aws-sdk": "^2.518.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-bootstrap": "^1.0.0-beta.12",
    "react-dom": "^16.9.0",
    "react-feather": "^2.0.3",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.1",
    "styled-components": "^4.3.2",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.1.0",
    "@testing-library/react": "^9.1.3",
    "cross-env": "^5.2.0",
    "eslint": "6.1.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "history": "^4.9.0",
    "prettier": "^1.18.2",
    "serverless-offline": "^5.10.1",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0"
  }
}
