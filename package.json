    "eject": "react-scripts eject",
    "lint": "eslint 'src/**/*.{js,jsx}' --ignore-pattern 'node_modules/*'"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
