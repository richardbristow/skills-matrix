{
  "name": "skills-matrix",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env REACT_APP_STAGE=dev react-scripts start",
    "build": "cross-env REACT_APP_STAGE=dev react-scripts build",
    "build:prod": "cross-env REACT_APP_STAGE=prod react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint 'src/**/*.{js,jsx}' 'backend/**/*.js' --ignore-pattern 'node_modules/*' && stylelint './src/**/*.{js,jsx}'",
    "lint:js": "eslint 'src/**/*.{js,jsx}' 'backend/**/*.js' --ignore-pattern 'node_modules/*'",
    "lint:css": "stylelint './src/**/*.{js,jsx}'"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!src/serviceWorker.js",
      "!src/setupTests.js",
      "!src/index.jsx"
    ]
  },
  "dependencies": {
    "aws-amplify": "^4.0.3",
    "aws-sdk": "^2.917.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.0",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.9",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "styled-components": "^5.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "cross-env": "^7.0.3",
    "eslint": "7.27.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "history": "^5.0.0",
    "prettier": "^2.3.0",
    "serverless-offline": "^7.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0"
  }
}
